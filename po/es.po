#: ../plugins/debug.py:55
msgid "dump information about installed rpm packages to file"
msgstr ""
"volcando información acerca de los los paquetes RPM instalados a un archivo"

#: ../plugins/debug.py:69
msgid "do not attempt to dump the repository contents."
msgstr "no intentar volcar el contenido del repositorio."

#: ../plugins/debug.py:72
msgid "optional name of dump file"
msgstr "nombre opcional del archivo de volcado"

#: ../plugins/debug.py:97
#, python-format
msgid "Output written to: %s"
msgstr "Salida escrita en: %s"

